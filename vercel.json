{
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "https://api.relife.kr/api/v1/:path*"
    }
  ],
  "redirects": [
    {
      "source": "/oauth2/authorization/:path*",
      "destination": "https://api.relife.kr/oauth2/authorization/:path*",
      "permanent": true
    }
  ]
}
